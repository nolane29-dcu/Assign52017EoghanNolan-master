<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eoghan.societygolf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Golfers" />
        <activity android:name=".AddGolfer" />
        <activity android:name=".DatabaseHelper" />
        <activity android:name=".GolferContract" />
        <activity android:name=".SQLiteListAdapter"></activity>
    </application>

</manifest>